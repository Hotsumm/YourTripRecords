k = () => {\n    const passwordRules = /^(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$%^&*-]).{8,16}$/;\n    if (!passwordRules.test(password)) {\n      alert('비밀번호는 8~16자 숫자/소문자/특수문자를 모두 포함해야 합니다.');\n      return;\n    }\n    handleSignUp();\n  };\n\n  const handleSignUp = async () => {\n    if (password !== passwordConfirm) {\n      alert('비밀번호를 확인해주세요.');\n      return;\n    }\n    try {\n      await firebaseAuth\n        .createUserWithEmailAndPassword(email, password)\n        .then(() => CreateUser(email, nickname));\n      //  setEmailConfirm((emailConfrim) => !emailConfirm);\n      alert('회원가입이 완료되었습니다.');\n      window.location.reload();\n    } catch (error) {\n      if (error.code === 'auth/weak-password') {\n        alert('비밀번호는 8자리 이상의 영문 + 특수문자로 입력해주세요.');\n        console.log(error.code);\n      } else if (error.code === 'auth/email-already-in-use') {\n        alert('이미 사용중인 이메일 입니다.');\n      } else if (error.code === 'auth/invalid-email') {\n        alert('이메일을 정확하게 입력해주세요.');\n      } else {\n        alert(error.code);\n      }\n    }\n  };\n\n  return (\n    <SignUpContainer>\n      <SignUpWrap>\n        <SignUpHeader>\n          <BsBoxArrowInLeft onClick={closeButton} size={26} />\n          <HeaderTitle>회원가입</HeaderTitle>\n        </SignUpHeader>\n        {!emailConfirm ? (\n          <>\n            <InputContainer>\n              <InputWrap>\n                <input\n                  type=\"email\"\n                  placeholder=\"이메일\"\n                  name=\"email\"\n                  onChange={onChange}\n                  required\n                />\n              </InputWrap>\n              <InputWrap>\n                <input\n                  type=\"text\"\n                  placeholder=\"닉네임\"\n                  name=\"nickname\"\n                  onChange={onChange}\n                  required\n                />\n              </InputWrap>\n              <InputWrap>\n                <input\n                  type=\"password\"\n                  placeholder=\"비밀번호\"\n                  name=\"password\"\n                  onChange={onChange}\n                  required\n                />\n              </InputWrap>\n              <InputWrap>\n                <input\n                  type=\"password\"\n                  placeholder=\"비밀번호 확인\"\n                  name=\"passwordConfirm\"\n                  onChange={onChange}\n                  required\n                />\n              </InputWrap>\n            </InputContainer>\n            <ButtonWrap>\n              <button onClick={validCheck}>회원가입</button>\n              <button>\n                <FcGoogle size={25} />\n                Google로 회원가입\n              </button>\n            </ButtonWrap>\n          </>\n        ) : (\n          <>\n            <InputContainer>\n              <InputWrap>\n                <h1> 이메일 코드를 입력해주세요.</h1>\n                <input type=\"text\" name=\"linkCode\" />\n              </InputWrap>\n            </InputContainer>\n            <ButtonWrap>\n              <button>확인</button>\n            </ButtonWrap>\n          </>\n        )}\n      </SignUpWrap>\n    </SignUpContainer>\n  );\n};\n\nexport default SignUp;\n",["112","113"],"/Users/sung-yeolha/Desktop/travel/src/routes/Home.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/App.js",[],"/Users/sung-yeolha/Desktop/travel/src/components/Navigation/NavSideBar.jsx",[],["114","115"],"/Users/sung-yeolha/Desktop/travel/src/firebaseConfig.js",[],"/Users/sung-yeolha/Desktop/travel/src/Context.js",[],["116","117"],"/Users/sung-yeolha/Desktop/travel/src/components/City/CityPost.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/routes/City.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/components/Auth/SignIn.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/components/Navigation/NavProfileBar.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/components/User/CreateUser.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/components/Navigation/NavSearchBar.jsx",[],"/Users/sung-yeolha/Des