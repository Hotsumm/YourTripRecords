 = () => setIsSignInClick(!isSignInClick);\n  const toggleSignUp = () => setIsSignUpClick(!isSignUpClick);\n  const toggleSignOut = () => setIsSignOutClick(!isSignOutClick);\n\n  return (\n    <>\n      <ProfileContent current={isUser} onClick={menuClick}>\n        {isUser ? (\n          <AvatarWrap>\n            <VscTriangleDown size={15} color={'grey'} />\n            <span>Hotsumm</span>\n            <HiUserCircle size={35} color={'grey'} />\n          </AvatarWrap>\n        ) : (\n          <>\n            <VscTriangleDown size={15} color={'grey'} />\n            <span>로그인을 해주세요.</span>\n            <HiUserCircle size={35} color={'grey'} />\n          </>\n        )}\n        {isActive && (\n          <ProfileMenu>\n            {isUser ? (\n              <ul>\n                <li>내 계정</li>\n                <li>여행기록 올리기</li>\n                <li>설정</li>\n                <li\n                  onClick={toggleSignOut}\n                  style={{ borderTop: '1px solid #ababab80' }}\n                >\n                  로그아웃\n                </li>\n              </ul>\n            ) : (\n              <ul>\n                <li onClick={toggleSignIn}>로그인</li>\n                <li onClick={toggleSignUp}>회원가입</li>\n              </ul>\n            )}\n          </ProfileMenu>\n        )}\n      </ProfileContent>\n      {isSignInClick && <SignIn toggleSignIn={toggleSignIn} />}\n      {isSignUpClick && <SignUp toggleSignUp={toggleSignUp} />}\n    </>\n  );\n};\n\nexport default NavProfile;\n","/Users/sung-yeolha/Desktop/travel/src/components/Auth/SignIn.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/components/Auth/SignUp.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/components/City/CityList.jsx",[],"/Users/sung-yeolha/Desktop/travel/src/utils/cityArray.js",[],{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","replacedBy":"71"},{"ruleId":"68","replacedBy":"72"},{"ruleId":"70","replacedBy":"73"},{"ruleId":"68","replacedBy":"74"},{"ruleId":"70","replacedBy":"75"},{"ruleId":